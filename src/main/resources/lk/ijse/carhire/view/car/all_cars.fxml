<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootnode" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.carhire.controller.car.AllCarsController">
   <children>
      <ImageView fitHeight="515.0" fitWidth="776.0">
         <image>
            <Image url="@../images/background_2.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tblCars" layoutX="39.0" layoutY="140.0" opacity="0.75" prefHeight="345.0" prefWidth="705.0">
         <columns>
            <TableColumn fx:id="colId" editable="false" prefWidth="52.0" text="Id" />
            <TableColumn fx:id="colMake" editable="false" prefWidth="113.0" text="Make" />
            <TableColumn fx:id="colCategory" editable="false" prefWidth="84.0" text="Category" />
            <TableColumn fx:id="colAvailability" editable="false" text="Availability" />
            <TableColumn fx:id="colRentPerDay" editable="false" prefWidth="95.0" text="Rent per day" />
            <TableColumn fx:id="colRent" editable="false" prefWidth="73.0" text="Rent" />
            <TableColumn fx:id="colDelete" editable="false" prefWidth="70.0" text="Delete" />
            <TableColumn fx:id="colRemarks" editable="false" prefWidth="137.0" text="Remarks" />
         </columns>
      </TableView>
      <Label layoutX="234.0" layoutY="14.0" prefHeight="56.0" prefWidth="294.0" text="CarHire Pvt Ltd" textFill="#e38b27">
         <font>
            <Font name="Britannic Bold" size="42.0" />
         </font>
      </Label>
      <Label layoutX="317.0" layoutY="54.0" text="All Cars" textAlignment="CENTER">
         <font>
            <Font name="Arial Black" size="30.0" />
         </font>
         <textFill>
            <LinearGradient endX="1.0" endY="0.11904761904761904" startX="1.0" startY="0.9619047619047619">
               <stops>
                  <Stop color="WHITE" />
                  <Stop color="#e58e26" offset="1.0" />
               </stops>
            </LinearGradient>
         </textFill>
      </Label>
      <Button layoutX="38.0" layoutY="29.0" mnemonicParsing="false" onAction="#btnDashboardOnAction" prefHeight="27.0" prefWidth="106.0" style="-fx-background-color: #e58e26; -fx-background-radius: 15;" text="Dashboard" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
         <graphic>
            <ImageView fitHeight="17.0" fitWidth="17.0">
               <image>
                  <Image url="@../icons/menu.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="38.0" layoutY="70.0" mnemonicParsing="false" onAction="#btnCarFormOnAction" prefHeight="27.0" prefWidth="106.0" style="-fx-background-color: #e58e26; -fx-background-radius: 15;" text="Car form" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
         <graphic>
            <ImageView fitHeight="18.0" fitWidth="20.0">
               <image>
                  <Image url="@../icons/form.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
